{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".output/**",
        "artifacts/**",
        "cache/**",
        "typechain-types/**"
      ]
    },
    "lint": {
      "dependsOn": ["$PRIVATE_KEY", "$OWNER_ADDRESS", "$LATEST_ADDRESS"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
